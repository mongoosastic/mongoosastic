{
  "author": "James R. Carr <james.r.carr@gmail.com> (http://blog.james-carr.org)",
  "name": "mongoosastic",
  "description": "A mongoose plugin that indexes models into elastic search",
  "version": "4.5.0",
  "tags": [
    "mongodb",
    "elasticsearch",
    "elastic search",
    "mongoose",
    "full text search"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mongoosastic/mongoosastic"
  },
  "main": "lib/mongoosastic.js",
  "dependencies": {
    "elasticsearch": "16.1.1",
    "lodash.clonedeep": "4.5.0"
  },
  "devDependencies": {
    "async": "3.0.1",
    "changelog": "1.4.2",
    "co-mocha": "1.2.2",
    "coveralls": "3.0.4",
    "eslint": "6.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "istanbul": "0.4.5",
    "mocha": "6.1.4",
    "mongoose": "5.6.0",
    "should": "13.2.3",
    "standard": "12.0.1"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "scripts": {
    "authors": "./scripts/update_authors.sh",
    "lint-fix": "npm run lint -- --fix",
    "lint": "eslint lib test",
    "mocha": "istanbul cover _mocha --report lcovonly -- test/*-test.js",
    "test": "npm run lint && npm run mocha",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "changelog": "changelog mongoosastic/mongoosastic latest -m"
  },
