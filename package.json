{
  "author": "James R. Carr <james.r.carr@gmail.com> (http://blog.james-carr.org)",
  "name": "mongoosastic",
  "description": "A mongoose plugin that indexes models into elastic search",
  "version": "5.0.1",
  "tags": [
    "mongodb",
    "elasticsearch",
    "elastic search",
    "mongoose",
    "full text search"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mongoosastic/mongoosastic"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "dependencies": {
    "@elastic/elasticsearch": "7.15.0",
    "lodash": "4.17.21",
    "mongoose": "6.0.13"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "coveralls": "3.1.1",
    "eslint": "8.2.0",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "scripts": {
    "authors": "./scripts/update_authors.sh",
    "prepare": "npm run build",
    "build": "rm -rf dist && tsc -p . ",
    "lint-fix": "npm run lint -- --fix",
    "lint": "eslint lib test",
    "jest": "jest --detectOpenHandles --runInBand",
    "test": "npm run lint && npm run jest",
    "coverage": "npm run test -- -- --coverage",
    "report": "cat coverage/lcov.info | coveralls",
    "changelog": "changelog mongoosastic/mongoosastic latest -m"
  },
  "license": "MIT"
}
