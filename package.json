{
  "author": "James R. Carr <james.r.carr@gmail.com> (http://blog.james-carr.org)",
  "name": "mongoosastic",
  "description": "A mongoose plugin that indexes models into elastic search",
  "version": "4.6.0",
  "tags": [
    "mongodb",
    "elasticsearch",
    "elastic search",
    "mongoose",
    "full text search"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mongoosastic/mongoosastic"
  },
  "main": "lib/mongoosastic.js",
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.14"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.5.6",
    "@types/node": "^14.6.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "jest": "^26.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "scripts": {
    "authors": "./scripts/update_authors.sh",
    "lint-fix": "npm run lint -- --fix",
